# platform :ios, '9.0'

target 'Peak' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod ‘Firebase/Database'
  pod ‘Firebase/Core'
  pod ‘Firebase/Auth'
  pod 'Firebase/Storage'

  pod 'Backendless'

  # Pods for Peak

  target 'PeakTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'PeakUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
